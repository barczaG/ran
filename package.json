{
  "name": "ranjs",
  "version": "1.7.4",
  "description": "Library for generating various random variables.",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "jstat": "^1.7.1",
    "mathjs": "^5.1.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "nyc": "^13.1.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/_mocha --require babel-core/register",
    "docs": "python docs/src/parser.py",
    "build": "webpack",
    "dev": "node_modules/nodemon/bin/nodemon.js -x 'npm run build' -w src/ran.js",
    "test2": "istanbul cover _mocha --report lcovonly -- -R spec",
    "test-old": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "coveralls": "istanbul cover ./node_modules/.bin/_mocha --require @babel/register",
    "cover": "nyc npm run test -- --grep @nycinvalid --invert && nyc report --reporter=text-lconv | coveralls"
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synesenom/ran"
  },
  "keywords": [
    "random",
    "generator",
    "distributions",
    "mcmc",
    "statistics",
    "test"
  ],
  "author": "Enys Mones <enys.mones@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/synesenom/ran/issues"
  },
  "homepage": "https://synesenom.github.io/ran/"
}
