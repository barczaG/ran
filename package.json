{
  "name": "ranjs",
  "version": "1.7.4",
  "description": "Library for generating various random variables.",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "jstat": "^1.7.1",
    "mathjs": "^5.1.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "chai": "^4.2.0",
    "nodemon": "^1.18.4",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "istanbul": "^0.4.5",
    "nyc": "^13.1.0",
    "eslint": "^5.12.1"
  },
  "scripts": {
    "lint": "eslint src/*.js",
    "test": "./node_modules/.bin/_mocha --require babel-core/register",
    "docs": "python docs/src/parser.py",
    "build": "webpack",
    "coverage": "nyc --reporter=text --reporter=html _mocha --require babel-core/register --timeout=3000",
    "coveralls": "nyc --reporter=text-lcov _mocha --require babel-core/register --timeout=3000 | coveralls"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "include": [
      "src/utils.js",
      "src/core.js",
      "src/la.js",
      "src/special.js",
      "src/ts.js",
      "src/mc.js",
      "src/dist.js",
      "src/index.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synesenom/ran"
  },
  "keywords": [
    "random",
    "generator",
    "distributions",
    "mcmc",
    "statistics",
    "test"
  ],
  "author": "Enys Mones <enys.mones@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/synesenom/ran/issues"
  },
  "homepage": "https://synesenom.github.io/ran/"
}
